@model IEnumerable<ShopBanHang.Models.DonHang>

@{
    ViewBag.Title = "DetailDonHang";
    Layout = "~/Views/Shared/_LayoutAdminCT.cshtml";
}




<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.daThanhToan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tinhTrang)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ngayDat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ngayGiao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KhachHang.tenKH)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.daThanhToan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tinhTrang)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ngayDat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ngayGiao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KhachHang.tenKH)
        </td>
        
    </tr>
}

</table>
